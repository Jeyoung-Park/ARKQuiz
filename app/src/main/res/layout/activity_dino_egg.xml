        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Button_dinobone200"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/TextView_dinobone2000"
